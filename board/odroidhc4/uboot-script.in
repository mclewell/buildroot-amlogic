setenv kernel_loadaddr "0x11000000"
setenv initrd_loadaddr "0x13000000"
setenv dtb_loadaddr "0x1000000"
setenv bootargs "console=ttyAML0,115200n8 console=tty0 no_console_suspend consoleblank=0 root=/dev/mmcblk0p2 rootwait rw"
fatload mmc 0:1 ${kernel_loadaddr} uImage
fatload mmc 0:1 ${dtb_loadaddr} meson-sm1-odroid-hc4.dtb
bootm ${kernel_loadaddr} - ${dtb_loadaddr}
